{
  "name": "@jill64/cf-tiktoken",
  "description": "‚è≥ js-tiktoken on Cloudflare Pages",
  "version": "0.1.1",
  "type": "module",
  "license": "MIT",
  "author": "jill64 <intents.turrets0h@icloud.com> (https://github.com/jill64)",
  "bugs": "https://github.com/jill64/cf-tiktoken/issues",
  "homepage": "https://github.com/jill64/cf-tiktoken#readme",
  "files": [
    "dist",
    "!*.test.*",
    "!*.spec.*"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "worker": "./dist/edge.js",
      "browser": "./dist/browser.js",
      "default": "./dist/edge.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jill64/cf-tiktoken.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "bpe",
    "cloudflare",
    "gpt",
    "llm",
    "pages",
    "tiktoken"
  ],
  "scripts": {
    "build": "tsc && npx publint",
    "prepack": "npm run build",
    "lint": "npx eslint . && npx depcheck",
    "format": "npx prettier --write ."
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "6.13.0",
    "@typescript-eslint/parser": "6.13.0",
    "eslint-plugin-deprecation": "2.0.0",
    "typescript": "5.3.2"
  },
  "dependencies": {
    "js-tiktoken": "1.0.8"
  }
}
